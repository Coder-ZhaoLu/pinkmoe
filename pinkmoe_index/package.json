    "pinia": "^2.0.16",
    "plop": "^3.1.1",
    "qs": "^6.11.0",
    "rollup": "^2.56.3",
    "swiper": "^8.3.2",
    "vite-ssg": "^0.20.2",
    "vue": "^3.2.37",
    "vue-i18n": "9",
    "vue3-aplayer": "^1.7.3",
    "vue3-photo-preview": "^0.2.5"
  },
